{
  "name": "nullforce-booru-extension",
  "version": "0.1.0",
  "description": "Extends the functionality of image booru sites. Integrates with Eagle DAM.",
  "scripts": {
    "clean": "pwsh -Command \"Remove-Item -Recurse -Force dist/*, .parcel-cache/*\"",
    "start": "parcel watch src/manifest.json",
    "build": "npm run clean && parcel build src/manifest.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "booru",
    "derpibooru",
    "manebooru",
    "chrome",
    "extension"
  ],
  "author": "nullforce",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "@types/chrome": "^0.0.202",
    "parcel": "^2.8.3",
    "typescript": "^4.9.3"
  }
}